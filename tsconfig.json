{
  "extends": "./path.json",
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
      "es2016.array.include",
      "ES2017.Object",
      "ES2020.Promise",
      "ES2017.String",
      "ESNext.String"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "sourceMap": true,
    "importHelpers": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "downlevelIteration": true,
    "noImplicitAny": false,
    "noImplicitThis": true,
    "allowUnreachableCode": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "typeRoots": [
    "node_modules/@types",
    "custom_typings"
  ],
  "include": [
    "src"
  ]
}
